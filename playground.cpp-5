#include <iostream>
#include <iomanip>

using namespace std;


int main() {

     int decimal = 15;
    int octal = 017;
    int hex = 0x0f;
    int binary = 0b00001111;

    cout << "Decimal: "     << decimal << endl;
    cout << "Octal: "         << octal << endl;
    cout << "Hexadecimal: " << hex << endl;
    cout << "Binary: "      << binary << endl;

}